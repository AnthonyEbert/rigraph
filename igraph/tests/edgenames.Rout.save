[2]  3 --  4
[3]  4 --  5
[4]  5 --  6
[5]  6 --  7
[6]  8 --  9
[7]  9 -- 10
> 
> ## mix edge names and vertex names
> g <- graph.ring(10)
> V(g)$name <- letters[1:vcount(g)]
> E(g)$name <- LETTERS[1:ecount(g)]
> delete.edges(g, c("a|b", "F", "j|i"))
