> library(igraph)
> 
> g <- graph.empty(10)
> g[1,2] <- TRUE
> g
IGRAPH D--- 10 1 -- 
+ edges:
[1] 1->2
> g[2,1] <- TRUE
> g
IGRAPH D--- 10 2 -- 
+ edges:
[1] 1->2 2->1
> g[2,1] <- NULL
> g
IGRAPH D--- 10 1 -- 
+ edges:
[1] 1->2
> g[1,2] <- FALSE
> g
IGRAPH D--- 10 0 -- 
> 
> g <- graph.empty(10)
> g[-1,1] <- 1
> g
IGRAPH D--- 10 9 -- 
+ edges:
[1]  2->1  3->1  4->1  5->1  6->1  7->1  8->1  9->1 10->1
> 
> ## Weighted graph
> 
> igraph.options(print.edge.attributes=TRUE)
> g <- graph.empty(10)
> g <- set.edge.attribute(g, "weight", c(), 1)
> g[1,2] <- 1
> g
IGRAPH D-W- 10 1 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2      1
> g[2,1] <- 2
> g
IGRAPH D-W- 10 2 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2      1
[2] 2->1      2
> g[1,2] <- 3
> g
IGRAPH D-W- 10 2 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2      3
[2] 2->1      2
> g[1:2,2:3] <- -1
> g
IGRAPH D-W- 10 5 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2     -1
[2] 2->1      2
[3] 1->3     -1
[4] 2->2     -1
[5] 2->3     -1
> g[1,2] <- NULL
> g
IGRAPH D-W- 10 4 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 2->1      2
[2] 1->3     -1
[3] 2->2     -1
[4] 2->3     -1
> 
> ## Using vertex names
> 
> g <- graph.empty(10)
> V(g)$name <- letters[1:vcount(g)]
> g['a','b'] <- TRUE
> g['b','c'] <- TRUE
> g
IGRAPH DN-- 10 2 -- 
+ attr: name (v/c)
+ edges (vertex names):
[1] a->b b->c
> g[c('a','f'),c('f','a')] <- TRUE
> g
IGRAPH DN-- 10 6 -- 
+ attr: name (v/c)
+ edges (vertex names):
[1] a->b b->c a->f a->a f->f f->a
> g[c('a','c','h'), c('a','a','a'), attr="weight"] <- 3
> g
IGRAPH DNW- 10 8 -- 
+ attr: name (v/c), weight (e/n)
+ edges and their attributes:
    edge weight
[1] a->b     NA
[2] b->c     NA
[3] a->f     NA
[4] a->a      3
[5] f->f     NA
[6] f->a     NA
[7] c->a      3
[8] h->a      3
