> all(degree(g) == degree(g3))
[1] TRUE
> all(degree(g) == degree(g2) / 2)
[1] TRUE
> 
> graph.isomorphic(g, as.undirected(g2))
[1] TRUE
> graph.isomorphic(g, as.undirected(g3))
[1] TRUE
